{
  "name": "base-project",
  "version": "1.0.0",
  "description": "A base project using Webpack, Express, React, React Router, Jest, Enzyme, and TypeScript",
  "main": "index.jsx",
  "scripts": {
    "test": "jest --watch",
    "build:client": "webpack --config webpack.client.config.js",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "rm -rf ./__build__ && npm run build:client -- --env.mode production && npm run build:server -- --env.mode production",
    "start": "node ./__build__/server.js",
    "start:dev": "rm -rf ./__build__ && (webpack-dev-server --config webpack.client.config.js --env.mode development & (just-wait -t 60 -p \"./__build__/public/index.html\" && npm run build:server -- --env.mode development))",
    "analyze:client": "rm -rf ./__build__ && npm run build:client -- --env.mode production --env.presets analyze"
  },
  "author": "Neal Higa",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "handlebars": "^4.0.12",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/compression": "0.0.36",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/express": "^4.16.0",
    "@types/handlebars": "^4.0.40",
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "^9.4.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^23.6.0",
    "just-wait": "^1.0.11",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.1",
    "react-hot-loader": "^4.3.11",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "start-server-webpack-plugin": "^2.2.5",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
